			</form>
		    <!-- キャンセルボタン -->
		    <a class="remodal-cancel" href="/studio_feestructure" data-remodal-action="cancel">戻る</a>
		    <!-- OKボタン -->
		    <a href="javascript:void(0)" class="remodal-confirm" onclick="document.studioFeestructureEdit.submit();return false;" style="display:inline-block">OK</a>
		</div>
	</div>
